<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="768.0" prefWidth="1024.0" style="-fx-background-color: #9EDF9C;" stylesheets="@styles.css" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.quizapplication.newquizhome">

    <Label layoutX="420.0" layoutY="100.0" style="-fx-font-size: 28px; -fx-font-weight: bold;" text="Add New Quiz" />


    <VBox layoutX="250.0" layoutY="200.0" spacing="10">
        <Label style="-fx-font-size: 18px; -fx-font-weight: bold;" text="Quiz Title:" />
    </VBox>


    <VBox layoutX="248.0" layoutY="301.0" spacing="10">
        <Label style="-fx-font-size: 18px; -fx-font-weight: bold;" text="Remarks:" />
        <TextField fx:id="remarksField" promptText="Enter Remarks" />
    </VBox>


    <Button fx:id="submitButton" layoutX="450.0" layoutY="400.0" onAction="#addingquizhome" style="-fx-font-size: 16px; -fx-font-weight: bold;" text="Submit" />
     <TextField fx:id="quizTitleField" layoutX="250.0" layoutY="254.0" prefHeight="26.0" prefWidth="165.0" promptText="Enter Quiz Title" />

</AnchorPane>
